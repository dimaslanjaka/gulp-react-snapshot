{
  "$schema": "http://json.schemastore.org/nodemon",
  "exec": "cross-env NODE_ENV=development DEBUG=prerender-it* run-s build test",
  "watch": [
    "./src/*.ts"
  ],
  "ignore": [
    "*.test.js",
    ".vscode",
    ".gitignore",
    "gh-pages",
    "dist",
    "tests",
    "tmp",
    "temp",
    "./src/tmp"
  ],
  "delay": 3000,
  "ext": "ts,js",
  "verbose": true
}