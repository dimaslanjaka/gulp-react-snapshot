{
  "name": "react-prerender-it",
  "version": "1.0.4",
  "description": "Generate static files from React production using Gulp",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "readme.md",
    "dist",
    "src",
    "LICENSE",
    "*.json"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc -p tsconfig.build.json",
    "test-build": "cd test && npm run update && cross-env-shell DISABLE_ESLINT_PLUGIN=true npm run build",
    "test": "cd test && cross-env-shell NODE_ENV=development DEBUG=prerender-it* node snapshot.js",
    "update": "npx npm-check-updates -u"
  },
  "keywords": [
    "react",
    "react-static",
    "hydrate",
    "puppeteer"
  ],
  "author": "Dimas Lanjaka <dimaslanjaka@gmail.com>",
  "repository": {
    "url": "https://github.com/dimaslanjaka/react-prerender-it",
    "type": "git"
  },
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.7.2",
    "express": "^4.18.2",
    "jsdom": "^20.0.1",
    "prettier": "^2.7.1",
    "puppeteer": "^19.0.0",
    "serve-static": "^1.15.0",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.37",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.8.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
